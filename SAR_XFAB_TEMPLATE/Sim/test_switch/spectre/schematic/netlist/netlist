
// Library name: SAR
// Cell name: switch
// View name: schematic
subckt switch CMD\+ CMD\- S\+ S\-
    M0 (S\+ CMD\+ S\- 0) ne3 w=wn/1 l=L  as=4.8e-07*(wn/1) \
        ad=4.8e-07*(wn/1) ps=1*2*(4.8e-07+(wn/1)) pd=2*(4.8e-07+(wn/1)) \
        nrs=2.7e-07/(wn/1) nrd=2.7e-07/(wn/1) m=(1)*(1) par1=(1)*(1) \
        xf_subext=0
    M1 (S\- CMD\- S\+ vdd!) pe3 w=wp/1 l=L as=4.8e-07*(wp/1) \
        ad=4.8e-07*(wp/1) ps=1*2*(4.8e-07+(wp/1)) pd=2*(4.8e-07+(wp/1)) \
        nrs=2.7e-07/(wp/1) nrd=2.7e-07/(wp/1) m=(1)*(1) par1=(1)*(1)
ends switch
// End of subcircuit definition.

// Library name: SAR
// Cell name: test_switch
// View name: schematic
V3 (net04 net6) isource dc=1u type=dc
V5 (net6 0) vsource dc=1.65 type=dc
V4 (vdd! 0) vsource dc=3.3 type=dc
V0 (net3 0) vsource dc=3.3 type=dc
E0 (R 0 net6 net04) vcvs gain=1000000
I2 (net3 0 net6 net04) switch
